<params>
    <!-- Nom de l'import -->
    <importname>Fichier texte -&gt; UNIMARC ISO 2709</importname>
    <!-- Nombre de notices traitées par passe -->
    <nperpass>1</nperpass>
    <!-- Type de l'entrée -->
    <input type="text" header="no"/>
    <!-- type de la sortie -->
    <output importable="yes" type="iso_2709" suffix="uni"/>
    <step type="texttoxml">
        <!-- Conversion texte en unimarc -->
        <trootelement>unimarc</trootelement>
        <separator>|</separator>
        <delimitedby></delimitedby>
        <escaped>yes</escaped>
        <cols>
            <col id="1">
                <field>010</field>
                <subfield>a</subfield>
            </col>
            <col id="2,3,4">
                <field>700</field>
                <subfield>a,b,4</subfield>
            </col>
            <col id="5,6,7">
                <field>701</field>
                <subfield>a,b,4</subfield>
            </col>
            <col id="8,9,10">
                <field>701</field>
                <subfield>a,b,4</subfield>
            </col>
            <col id="11,12,13">
                <field>702</field>
                <subfield>a,b,4</subfield>
            </col>
            <col id="14,15,16">
                <field>702</field>
                <subfield>a,b,4</subfield>
            </col>
            <col id="17,18,19">
                <field>702</field>
                <subfield>a,b,4</subfield>
            </col>
            <col id="20,21,22">
                <field>702</field>
                <subfield>a,b,4</subfield>
            </col>
            <col id="23,24,25">
                <field>702</field>
                <subfield>a,b,4</subfield>
            </col>
            <col id="26,27,28">
                <field>702</field>
                <subfield>a,b,4</subfield>
            </col>

            <col id="29,30,31,32">
                <field>200</field>
                <subfield>a,c,d,e</subfield>
                <ind>1 </ind>
            </col>
            <col id="33">
                <field>205</field>
                <subfield>a</subfield>
            </col>
            <col id="34,35,36">
                <field>210</field>
                <subfield>a,c,d</subfield>
            </col>
            <col id="37,38,39,40">
                <field>225</field>
                <subfield>a,v,x,i</subfield>
            </col>
            <col id="41,42">
                <field>461</field>
                <subfield>t,v</subfield>
            </col>
            <col id="43">
                <field>676</field>
                <subfield>a</subfield>
            </col>
            <col id="44">
